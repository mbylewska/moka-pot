{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "settings": {
        "layout": {
            "contentSize": "1183px",
            "wideSize": "1183px"
        },
        "color": {
            "palette": [
                {
                    "slug": "moka-black",
                    "color": "#30291F",
                    "name": "black"
                },
                {
                    "slug": "moka-brown",
                    "color": "#C8A287",
                    "name": "brown"
                },
                {
                    "slug": "moka-white",
                    "color": "#FFFBF5",
                    "name": "white"
                },
                {
                    "slug": "moka-pink",
                    "color": "#CFB4AC",
                    "name": "pink"
                },
                {
                    "slug": "moka-dark-pink",
                    "color": "#C85676",
                    "name": "dark pink"
                }
                
            ]
        },
        "typography": {
            "fontFamilies": [{
                "fontFamily": "'Fraunces', serif",
                "slug": "fraunces",
                "name": "Headings-Fraunces",
                "fontFace": [{
                    "fontFamily": "'Fraunces', serif",
                    "fontWeight": "400",
                    "fontStyle": "normal",
                    "fontStretch": "normal",
                    "src": ["file:./assets/fonts/Fraunces_72pt_Soft-Regular.ttf"]
            
                }]
            },
            {
                "fontFamily": "'Roboto', sans-serif",
                "slug": "roboto",
                "name": "body-Roboto",
                "fontFace": [{
                    "fontFamily": "'Roboto', sans-serif",
                    "fontWeight": "400",
                    "fontStyle": "normal",
                    "fontStretch": "normal",
                    "src": ["file:./assets/fonts/Roboto-Regular.ttf"]
            
                }]
            }
        ]
        }
    },
    "styles": {
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--roboto)",
            "fontWeight": "1.5",
            "fontSize": "1rem"
        },
        "elements": {
            "h1": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--fraunces)",
                    "fontSize": "50px"
                }
            }
        }
    }


}